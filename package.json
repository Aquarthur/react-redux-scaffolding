  "description": "Basic scaffolding for generic React/Redux projects.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch webpack.config.js --exec \"webpack serve --mode development\"",
    "build": "webpack --mode production",
    "build:stats": "webpack --mode production --profile --progress --json > stats.json",
